# Copy this file to .env and fill in your credentials
# DO NOT commit the .env file to version control

# Spotify API Credentials
# Get these from: https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Icecast Passwords
# Set a strong password for the source connection
ICECAST_SOURCE_PASSWORD=your_icecast_source_password_here

# Optional: Icecast admin passwords
ICECAST_ADMIN_PASSWORD=admin_password_here
ICECAST_PASSWORD=listener_password_here
ICECAST_RELAY_PASSWORD=relay_password_here
